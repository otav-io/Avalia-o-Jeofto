// Dado um número positivo, crie um programa que escreva todos os números ímpares
//menores e/ou iguais a esse número e maiores ou igual a um
#include <stdio.h>
#include <stdlib.h>
   
int main(){
  int num;
 
 //Entrada
  printf("Digite um número inteiro positivo: ");
  scanf("%d", &num);  
 
 //Processamento
  while(num < 0){
    printf("Digite um número inteiro positivo: ");
    scanf("%d", &num);
  }  
 
 //Saída
  if(num % 2 == 0){
    num = num - 1; 
  }
